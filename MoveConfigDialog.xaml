<Window x:Class="đồ_án_1___interface.MoveConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:đồ_án_1___interface"
        mc:Ignorable="d"
        Title="MoveConfigDialog" Height="200" Width="250">
    <Canvas>
        <StackPanel Canvas.Left="63" Canvas.Top="22">
            <Label FontWeight="Bold">Select mode</Label>
            <RadioButton Name="front" IsChecked="True">Move to the front</RadioButton>
            <RadioButton Name="back">Move to the back</RadioButton>
        </StackPanel>
        <Button Name="okButton" Width="80" Height="25" Content="OK" Click="OkButton_Click" Canvas.Left="34" Canvas.Top="103"/>
        <Button Content="Cancel" Width="80" Height="25" IsCancel="True" Canvas.Left="128" Canvas.Top="103"/>
    </Canvas>
</Window>
